G12 = 0 $ G13 = 0 $ G23 = 0 
PR12 = 0 $ PR13 = 0 $ PR23 = 0
S11t = 0 $ S11c = 0 $ S22t = 0 $ S22c = 0 $ S33t = 0 $ S33c = 0
S12 = 0 $ S23 = 0 $ S13 = 0
